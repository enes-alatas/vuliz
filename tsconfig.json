{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "es6",
    "lib": ["dom", "es6", "es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "build",
    "paths": {
      "src/*": ["src/*"]
    },
  },
  "exclude": [
    "node_modules",
  ]
}
