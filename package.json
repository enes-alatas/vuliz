{
  "name": "vuliz",
  "version": "0.0.1",
  "description": "A simple vulnerability visualizer for your dependencies",
  "main": "index.js",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "build": "webpack --mode production",
    "start": "webpack serve --mode development --open",
    "dev": "webpack watch --mode development",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enes-alatas/vuliz.git"
  },
  "keywords": [
    "vulnerability",
    "visualizer"
  ],
  "author": "Enes Oguzhan Alatas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enes-alatas/vuliz/issues"
  },
  "homepage": "https://github.com/enes-alatas/vuliz#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "buffer": "^6.0.3",
    "gts": "^6.0.2",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "util": "^0.12.5",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5"
  }
}
